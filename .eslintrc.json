{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "jest/globals": true
  },
  "extends": ["airbnb", "prettier", "plugin:jest/recommended"],
  "rules": {
    "no-plusplus": 0,
    "consistent-return": 0,
    "no-console": 0,
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "no-param-reassign": 0
  },
  "plugins": ["jest"]
}
